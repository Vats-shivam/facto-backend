{
  "name": "homy-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "set NODE_ENV=dev&& ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/server.ts",
    "start:uat": "npm run build-ts && set NODE_ENV=uat && node ./dist/server.js",
    "start:prod": "npm run build-ts:prod && NODE_ENV=prod node ./dist/server.js",
    "test": "jest --forceExit --coverage --verbose",
    "check-format": "npx prettier -c src",
    "format": "npx prettier -w src",
    "build-ts": "tsc -p tsconfig.release.json || npm run copy",
    "copy": "cp package.json ./dist/ && cp pm2.config.uat.json ./dist/pm2.config.json && cp .env.uat ./dist/",
    "build-ts:prod": "tsc -p tsconfig.release.json || npm run copy:prod",
    "copy:prod": "cp package.json ./dist/ && cp pm2.config.prod.json ./dist/pm2.config.json && cp .env.prod ./dist/",
    "start:production-dummy": "set NODE_ENV=prod&& ts-node-dev --respawn --transpile-only ./src/server.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "_moduleAliases": {
    "@": "src",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@interfaces": "src/interfaces",
    "@config": "src/config",
    "@utils": "src/utils"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/winston": "^2.4.4",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-http-context": "^1.2.4",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.7.3",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "razorpay": "^2.9.5",
    "uuid": "^10.0.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
